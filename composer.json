    "homepage": "https://github.com/Deisss/php-slim-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Deisss",
            "homepage": "http://simplapi.wordpress.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.*"
    },
    "autoload": {
        "psr-0": {
            "AbstractHTTPBasicAuth": "."
        }
    }
}